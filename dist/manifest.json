{
  "manifest_version": 2,
  "name": "ry-wow",
  "description": "Assistant for WOW Data Searching",
  "version": "1.0",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [

  ],
  "background": {
    "scripts": [
      "js/background.js",
      "js/hot-reload.js"
    ]
  },
  "permissions": [
    "storage"
  ]
}